{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "eqeqeq": "error",
    "no-debugger": "error"
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "no-debugger": ["error", { "custom-rules/no-debugger": "error" }]
      }
    }
  ]
}
